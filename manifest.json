{
  "manifest_version": 3,
  "name": "Hello",
  "description": "Тестовое расширение. Го погнали!",
  "version": "1.0.0",
  "icons": {
    "192": "logo-loader.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://123.frfrstaging.pw/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "action": {
    "default_icon": "logo-loader.png",
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "activeTab",
    "storage",
    "scripting"
  ]
}
